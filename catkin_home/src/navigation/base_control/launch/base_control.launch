<launch>
    <master 
        auto="start"
    />
    <!-- Run the map server --> 
    <!-- <arg 
        name="map_file" 
        default="$(find nav_main)/maps/TMR_2019.yaml"
    />
    <node 
        name="map_server" 
        pkg="map_server" 
        type="map_server" 
        args="$(arg map_file)" 
    />
    <node 
        pkg="tf" 
        type="static_transform_publisher" 
        name="odom_broadcaster" 
        args="0 0 0 0 0 0 1 map odom 20" 
    /> -->
    <!-- Run the arduino serial communication node. --> 
    <node 
        pkg="rosserial_python" 
        type="serial_node.py" 
        name="rosserial" 
        output="screen"
    >
        <param 
            name="port" 
            value="/dev/ttyACM0" 
        />
        <param 
            name="baud" 
            value="57600" 
        />
    </node>
    <!-- Run the odometry calculator node. 
    <node
        pkg="base_control"
        name="base_odom"
        type="base_odom"
    /> -->
    <!-- Run the velocity commands simulation node. --> 
    <!-- <node
        pkg="base_control"
        name="cmd_vel_simulation"
        type="cmd_vel.py"
    />
    <!-- Send CmdVel Node. --> 
    <node
        pkg="teleop_twist_keyboard"
        type="teleop_twist_keyboard.py"
        name="teleop"
        output="screen"
    />
    <!-- Run rviz simulation. --> 
    <include 
        file="$(find robot_description)/launch/display.launch" 
    /> -->
    <!--<node 
        type="rviz" 
        name="rviz" 
        pkg="rviz" 
        args="-d $(find base_control)/rviz/base_control.rviz" 
    />-->
 </launch>